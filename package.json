{
  "name": "norish",
  "version": "0.14.0-beta",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.26.0",
  "scripts": {
    "dev": "tsx watch server.ts | pnpm exec pino-pretty",
    "build": "next build && pnpm run build:server && pnpm run update-sw",
    "build:server": "tsdown",
    "update-sw": "node scripts/update-sw-version.js",
    "clean:install": "pnpm install --frozen-lockfile",
    "build:prod": "pnpm run clean:install && cross-env NODE_ENV=production pnpm run build",
    "db:ensure": "tsx db/scripts/ensure-db.ts",
    "db:push": "pnpm run db:ensure && drizzle-kit push --config ./server/db/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config ./server/db/drizzle.config.ts",
    "migrate": "pnpm run db:ensure && drizzle-kit push --config ./server/db/drizzle.config.ts",
    "docker:build": "docker build -f docker/Dockerfile -t norishapp/norish:latest .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "start": "node dist-server/server.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@trpc/tanstack-react-query": "^11.0.0",
    "better-auth": "^1.4.7",
    "bullmq": "^5.65.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "heic-convert": "^2.1.0",
    "html-entities": "^2.6.0",
    "ioredis": "^5.8.2",
    "next": "16.0.10",
    "pg": "8.16.3",
    "pino": "^10.1.0",
    "playwright-core": "npm:rebrowser-playwright-core@^1.52.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4",
    "superjson": "^2.2.6",
    "yt-dlp-wrap": "^2.3.12",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@floating-ui/react-dom": "2.1.6",
    "@heroicons/react": "2.2.0",
    "@heroui/react": "^2.8.6",
    "@iconify/react": "^6.0.2",
    "@mozilla/readability": "0.6.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.1",
    "@types/heic-convert": "^2.1.0",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "array-flatten": "3.0.0",
    "autoprefixer": "^10.4.23",
    "cheerio": "1.1.2",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.8",
    "eslint": "9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "globals": "^16.5.0",
    "idb-keyval": "6.2.2",
    "jose": "6.1.3",
    "jsdom": "^27.3.0",
    "jsonrepair": "3.13.1",
    "jszip": "3.10.1",
    "microdata-node": "2.0.0",
    "mime": "4.1.0",
    "motion": "12.23.26",
    "next-themes": "0.4.6",
    "openai": "5.23.2",
    "openid-client": "6.8.1",
    "parse-ingredient": "1.3.1",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-virtuoso": "4.17.0",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.18.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "uuid": "13.0.0",
    "vitest": "^4.0.16",
    "ws": "8.18.3",
    "zustand": "5.0.9"
  },
  "overrides": {
    "zod": "4.2.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
